{
  "name": "proto",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@prisma/client": "^5.4.2",
    "clsx": "^2.0.0",
    "jsonwebtoken": "^9.0.2",
    "next": "13.5.4",
    "prisma": "^5.4.2",
    "react": "^18",
    "react-dom": "^18",
    "react-icons": "^4.11.0",
    "sass": "^1.69.2",
    "zod": "^3.22.4"
  },
  "eslintConfig": {
    "rules": {
      "no-unreachable": "error",
      "no-unused-vars": [
        "error",
        {
          "vars": "all",
          "args": "none"
        }
      ],
      "no-var": "error",
      "default-case": "error",
      "array-callback-return": "error",
      "no-sparse-arrays": "error",
      "jsx-a11y/anchor-is-valid": [
        "error",
        {
          "aspects": [
            "noHref"
          ]
        }
      ]
    }
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.3",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10",
    "eslint": "^8",
    "eslint-config-next": "13.5.4",
    "postcss": "^8",
    "prettier": "^3.0.3",
    "tailwindcss": "^3",
    "typescript": "^5"
  }
}
